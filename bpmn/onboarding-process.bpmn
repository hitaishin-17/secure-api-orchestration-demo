<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  id="Definitions_1"
                  targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="OnboardingProcess" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Start Onboarding"/>
    <bpmn:task id="Task_VerifyDocuments" name="Verify Documents"/>
    <bpmn:task id="Task_CreateAccount" name="Create Account"/>
    <bpmn:endEvent id="EndEvent_1" name="Onboarding Complete"/>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_VerifyDocuments"/>
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_VerifyDocuments" targetRef="Task_CreateAccount"/>
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_CreateAccount" targetRef="EndEvent_1"/>
  </bpmn:process>
</bpmn:definitions>
